{
  "name": "vantatrack-ad-engine",
  "version": "1.0.0",
  "description": "Complete Media Management for Bangladesh Agencies",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
