    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "drizzle-orm": "^0.29.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "express-session": "^1.17.3",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-jwt": "^4.0.1",
    "connect-pg-simple": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
